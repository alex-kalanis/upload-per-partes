{
    "name": "alex-kalanis/upload-per-partes",
    "description": "HTTP Uploading Client for PHP",
    "keywords": [
        "http",
        "upload",
        "uploading",
        "sequential",
        "step-by-step"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "alex-kalanis/kw_storage": "~5.0",
        "alex-kalanis/kw_files": ">=4.1 <5",
        "alex-kalanis/kw_paths": ">=4.0 <5",
        "psr/container": "2.0.2",
        "php": ">=7.4.0",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "suggest": {
        "predis/predis": "Redis client"
    },
    "require-dev": {
        "predis/predis": "^2.2",
        "furious/psr7": "^1.3",
        "shipmonk/composer-dependency-analyser": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": ">8.0 <=9"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\UploadPerPartes\\": "php-src/"
        }
    },
    "scripts": {
    },
    "authors": [
        {
            "name": "Petr Plsek",
            "homepage": "https://www.kalanys.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
