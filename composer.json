{
    "name": "alex-kalanis/upload-per-partes",
    "description": "HTTP Uploading Client for PHP",
    "keywords": [
        "http",
        "upload",
        "uploading",
        "sequential",
        "step-by-step"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "suggest": {
        "predis/predis": "Redis client",
        "alex-kalanis/kw_storage": "Universal storage library for store data as key-value pairs"
    },
    "require-dev": {
        "alex-kalanis/kw_storage": "~3.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": ">7.2 <=9"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\UploadPerPartes\\": "php-src/"
        }
    },
    "scripts": {
    },
    "authors": [
        {
            "name": "Petr Plsek",
            "homepage": "http://www.kalanys.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
